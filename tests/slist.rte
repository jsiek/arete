fun listof(data) {
  return new data, null;
}

fun push(data, !list) {
  return new data, 1 of list;
}

var head = 0;
var tail = 1;

fun is_last(node) {
  if (is_null(node)) {
    return false;
  } else {
    return is_null(node[tail]);
  }
}

// Split the list into two parts, with the second
// part just containing the last element.
fun remove_last(!n) {
  if (is_null(n)) {
    return 0;
  } else {
    if (is_last(n[tail])) {
      var !q = n[tail];
      n[tail] = null;
      return q;
    } else {
      return remove_last(n[tail]);
    }
  }
}

fun main() {
  var !one_two = push(1, listof(2));
  var !two_null = remove_last(one_two);
  var one = one_two[head];
  var two = two_null[head];
  return one + one - two;
}