// This program is nondeterministic. 

fun write1(@q) {
  while (not (upgrade(&q))) { }
  *q = 1;
}

fun write2(@q) {
  while (not (upgrade(&q))) { }
  *q = 2;
}

fun main() {
  def !p = new 0;
  def t1 = spawn write1(p);
  def t2 = spawn write2(p);
  ! wait t2;
  ! wait t1;
  if (*p == 1) {
    return 0;
  } else if (*p == 2) {
    return 0;
  } else {
    return 1;
  }
}
