// This program is nondeterministic. 

fun write1(@q) {
  while (not (upgrade(q))) { }
  *q = 1;
}

fun write2(@q) {
  while (not (upgrade(q))) { }
  *q = 2;
}

fun main() {
  let !p = new 0;
  let t1 = future write1(p);
  let t2 = future write2(p);
  ! await t2;
  ! await t1;
  if (*p == 1) {
    return 0;
  } else if (*p == 2) {
    return 0;
  } else {
    return 1;
  }
}