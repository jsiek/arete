fun front(let ls) & { return ls[0]; }

fun rest(let ls) & { return ls[1]; }

fun map(let f, let ls) {
  if (is_null(ls)) {
    return null;
  } else {
    return ⟨f(front(ls)), map(f, rest(ls))⟩;
  }
}

fun inc(let x) { return x + 1; }

fun main() {
  var ls1 = ⟨0, ⟨1, ⟨2, null⟩⟩⟩;
  var ls2 = map(inc, ls1);
  let three = front(rest(rest(ls2)));
  return three - 3;
}
